{
    "name": "@g4/schemas",
    "description": "Shared Zod schemas",
    "author": "Joshua Dave E. Oropilla <joshdave0915@gmail.com>",
    "license": "MIT",
    "version": "1.0.0",
    "private": true,
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      },
      "./iam": {
        "types": "./src/iam/index.ts",
        "import": "./dist/iam/index.mjs",
