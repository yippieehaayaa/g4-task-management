      "watch": "tsup --watch"
    },
    "dependencies": {
      "zod": "^4.3.6"
    },
    "devDependencies": {
      "@g4/typescript-config": "*",
      "tsup": "^8.5.1",
      "typescript": "5.9.2"
    }
  }