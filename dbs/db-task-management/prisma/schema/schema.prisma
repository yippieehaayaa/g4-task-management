generator client {
    provider      = "prisma-client-js"
    output        = "../../generated/client"
    binaryTargets = ["native", "debian-openssl-3.0.x", "linux-musl-arm64-openssl-3.0.x"]
    engineType    = "library"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}
